<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title">{{ titre }} <span class="badge">{{ liste|length }}</span></h3>
	</div>
	<div class="panel-body">
		{% for key,elem in liste %}
			{% if elem is iterable %}
				<p>{% include 'ensemble01filemakerBundle:blocs:recursive-liste.html.twig' with {liste: elem, titre: key} %}</p>
			{% else %}
				<p>{{ elem }}</p>
			{% endif %}
		{% endfor %}
	</div>
</div>
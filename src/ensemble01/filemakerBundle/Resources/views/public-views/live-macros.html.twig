{% macro blocRapport(rapport, ligne1, ligne2) %}
	<div style="height:64px;width:100%;overflow:hidden">
		{{ ligne1|default(rapport.getField("rapport_nom"))|raw }}
	</div>
	<div style="height:24px;width:100%;overflow:hidden">
		<small>{{ ligne2|default(rapport.getField("type_rapport"))|raw }}</small>
	</div>
{% endmacro %}

